{
  "name": "webgl",
  "version": "0.0.0",
  "repository": " ",
  "author": "Jack Boyce",
  "main": "server.js",
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server --config config/webpack/webpack.config.dev.js --history-api-fallback --progress --port 3000",
    "build": "webpack --config config/webpack/webpack.config.prod.js",
    "build:gh": "npm run build && ./config/git-gh.sh"
  },
  "dependencies": {
    "global": "^4.3.0",
    "glsl-aastep": "^1.0.1",
    "glsl-checker": "^1.0.1",
    "glsl-noise": "0.0.0",
    "glslify": "^5.0.0",
    "glslify-fancy-imports": "^1.0.1",
    "glslify-hex": "^2.0.1",
    "interpol-js": "1.0.2",
    "leapjs": "^0.6.4",
    "leapjs-plugins": "^0.1.11",
    "mesh-reindex": "^1.0.0",
    "rx": "4.1.0",
    "three": "0.77.0",
    "three-buffer-vertex-data": "^1.0.1",
    "three-orbit-controls": "72.0.0",
    "unindex-mesh": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "6.3.26",
    "babel-loader": "6.2.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-es2015-native-modules": "^6.6.0",
    "babel-preset-stage-2": "6.11.0",
    "file-loader": "^0.9.0",
    "glslify-loader": "^1.0.2",
    "ify-loader": "mattdesl/ify-loader#fix-errors",
    "raw-loader": "^0.5.1",
    "webpack": "2.1.0-beta.4",
    "webpack-config": "5.2.1",
    "webpack-dev-server": "2.0.0-beta"
  },
  "browserify": {
    "transform": [
      "glslify"
    ]
  },
  "glslify": {
    "transform": [
      "glslify-fancy-imports",
      "glslify-hex"
    ]
  }
